	$ ( документ ). готов (() => {  
	// Загрузка скрипта для обработки шаблонов
	$ ( «тело» ). добавить ( '<script src="https://cdn.jsdelivr.net/npm/handlebars@latest/dist/handlebars.js"></script>' );
 
	// Добавление кнопок при включении страницы
	addButton ( 'На рассмотрении' , 'pin' , 'border-radius: 13px;margin-right: 5px; border: 2px сплошной; border-color: rgb(203, 40, 33, 0.5);' );  
    addButton ( 'Отказано' , 'unaccept' , 'border-radius: 13px; Margin-right: 5px; border: 2px Solid; border-color: rgb(255, 36, 0, 0.5);' )  
    addButton ( 'Одобрено' , 'принято' , 'border-radius: 13px;margin-right: 5px; border: 2px сплошной; border-color: rgb(152, 251, 152, 0.5);' )  
	addButton ( 'Закрыто' , 'закрыто' , 'border-radius: 13px;margin-right: 5px; border: 2px сплошной; border-color: rgb(138, 43, 226, 0.5);' );  
	добавитьОтветы ();
	
	// Поиск информации по теме
	const threadData = getThreadData ();
 
	$ (` кнопка # ff` ). щелкните (() => PasteContent ( 8 , threadData , true ));  
	$ (` кнопка # prr` ). щелкните (() => PasteContent ( 2 , threadData , true ));  
	$ (` кнопка # жб` ). нажмите (() => PasteContent ( 21 , threadData , true ));  
	$ ( 'кнопка#непринять' ). нажмите (() => editThreadData ( UNACCEPT_PREFIX , false ));  
	$ ( 'кнопка#принято' ). нажмите (() => editThreadData ( AC С EPT_PREFIX , false ));  
	$ ( 'кнопка#контакт' ). щелкните (() => editThreadData ( PIN_PREFIX , true ));  
	$ ( 'кнопка#teamProject' ). щелкните (() => editThreadData ( COMMAND_PREFIX , true ));  
	$ ( 'кнопка#просмотрено' ). щелкните (() => editThreadData ( WATCHED_PREFIX , false ));  
	$ ( 'кнопка#решено' ). нажмите (() => editThreadData ( DECIDED_PREFIX , false ));  
	$ ( 'кнопка#закрыто' ). щелкните (() => editThreadData ( CLOSE_PREFIX , false ));  
	$ ( 'кнопка#закрытая_жалоба' ). щелкните (() => editThreadData ( CLOSE_PREFIX , false , false ));   
	$ ( 'кнопка#техническая спецификация' ). щелкните (() => editThreadData ( TEX_PREFIX , true ));  
 
	$ (` кнопка # selectAnswer` ). нажмите (() => {  
	ХФ . alert ( buttonsMarkup ( buttons ), null , 'ВЫБЕРИТЕ ОТВЕТ' );  
	кнопки . forEach (( btn , id ) => {  
	если ( идентификатор > 1 ) {   
	$ (` кнопка # отвечает - $ { id }`). щелкните (() => PasteContent ( id , threadData , true ));  
	}
	еще { 
	$ (` кнопка # отвечает - $ { id }`). нажмите (() => PasteContent ( id , threadData , false ));  
	}
	});
	});
	});
 
    функция addButton ( имя , идентификатор , стиль ) { 
         $ ( '.button--icon--reply' ). до (
	`< button type = "button" class = "button--primary button rippleButton" id = "${id}" style = "${style}" > $ { name </ button >`, 
	);
	}
	функция addAnswers () { 
		$ ( '.button--icon--reply' ). after (`< button type = "button" class = "button--cta uix_quickReply--button button button--icon button--icon-write rippleButton" id = "selectAnswer" style = "oswald: 3px; Margin-left : 5px; border-top: 10px; border-radius: 13px;" >ОТВЕТЫ</ button >`, 
	);
	}
 
	функциональные кнопкиРазметка ( кнопки ) { 
	return `< div class = "select_answer" > $ { button 
	. карта (
	( кнопка , я ) => 
	`< button id = "ответы-${i}" class = " button -- основная кнопка ` +  
	` rippleButton "style=" поля : 4px ; $ { батн . dpstyle } "><span class=" button - text "> $ { btn . title </span> </ button >`,
	)
	. присоединиться ( '' )</ div >`;
	}
 
	функция PasteContent ( id , данные = {}, отправить = ложь ) {   
	константный шаблон = Руль . скомпилировать ( кнопки [ id ] .content ); 
	if ( $ ( '.fr-element.fr-view p' ). text () === '' ) $ ( '.fr-element.fr-view p' ). пустой ();   
 
	$ ( 'span.fr-placeholder' ). пустой ();
	$ ( 'div.fr-element.fr-view p' ). добавить ( шаблон ( данные ));
	$ ( 'a.overlay-titleCloser' ). триггер ( «щелчок» );
 
	если ( отправить == правда ) {   
	editThreadData ( кнопки [ id ].prefix , кнопки [ id ] .status ) ;
	$ ( '.button--icon.button--icon--reply.rippleButton' ). триггер ( «щелчок» );
	}
	}
 
	функция getThreadData () { 
	constauthorID = $ ( 'a.username' ) [ 0 ]. атрибуты [ 'идентификатор-пользователя-данных' ]. значение узла ;
	constauthorName = $ ( 'a.username' ) . HTML ();
	константные часы = новая дата (). Получить часы ();  
	возвращаться { 
	пользователь : { 
	идентификатор : идентификатор автора ,
	имя : Имя автора ,
	упоминание : `[ USER = $ { authorID }] $ { authorName }[/ USER ]`, 
	},
	приветствие : () =>  
	4 < часы && часы <= 11 ?   
	'Доброе утро' : 
	11 < часов && часов <= 15 ?   
	'Добрый день' : 
	15 < часы && часы <= 21 ?   
	'Добрый вечер' : 
	'Доброй ночи' ,
	};
	}
          функция editThreadData ( префикс , вывод = false ) {  
          // Получаем заголовок темы, так как он необходим при запросе
            const threadTitle = $ ( '.p-title-value' )[ 0 ]. последний ребенок . текстСодержимое ;
           
            если ( штифт == ложь ) { 
              выборка (` $ { документ . URL } редактировать `, { 
                метод : 'POST' , 
                тело : getFormData ({
                prefix_id : префикс ,
                заголовок : ThreadTitle ,
                _xfToken : XF . конфигурация . csrf ,
                _xfRequestUri : документ . URL-адрес . раскол ( XF . config . url.fullBase ) [ 1 ] ,
                _xfWithData : 1 , 
                _xfResponseType : 'json' , 
                }),
              }). затем (() => location . reload ()); 
            }
            если ( контактный == верно ) { 
              выборка (` $ { документ . URL } редактировать `, { 
                метод : 'POST' , 
                тело : getFormData ({
                prefix_id : префикс ,
                заголовок : ThreadTitle ,
                липкий : 1 , 
                _xfToken : XF . конфигурация . csrf ,
                _xfRequestUri : документ . URL-адрес . раскол ( XF . config . url.fullBase ) [ 1 ] ,
                _xfWithData : 1 , 
                _xfResponseType : 'json' , 
                }),
              }). затем (() => location . reload ()); 
            }
          }
           
          функция getFormData ( данные ) { 
            const formData = новые FormData ();  
            Объект . записи ( данные ). forEach ( я => formData.добавление ( я [ 0 ] , я [ 1 ] ));
            вернуть данные формы ;
            }
          })();
